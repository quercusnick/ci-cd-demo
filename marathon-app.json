{
  "id": "demo",
  "cmd": "python3 -m http.server 8080",
  "cpus": 1,
  "mem": 128.0,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "quercusnick/ci-cd-demo:latest",
      "network": "BRIDGE",
      "portMappings": [
        {
        "containerPort": 8080,
        "hostPort": 0,
        "servicePort": 9999,
        "protocol": "tcp"
         }
       ],
    }
  }
}
